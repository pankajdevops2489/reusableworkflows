pipeline {
    agent none
    stages {
        stage('Hello') {
            agent { 
                label "${env.GIT_BRANCH == 'main' ? 'aws' : 'aws-dev'}"
            }
            steps {
                echo "Running on ${env.GIT_BRANCH == 'main' ? 'aws' : 'aws-dev'}"
                echo 'Hello World'
            }
        }
    }
}
